    "def split_target(train_data):\n",
    "    train_data.dropna(axis = 0, subset = [\"loan_paid_back\"], inplace = True)\n",
    "    target = train_data.pop(\"loan_paid_back\")\n",
    "    return train_data, target\n",
    "\n"
   ]
