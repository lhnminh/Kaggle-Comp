    "train_data, validation_data = load_data_set(train,validation)\n",
    "train_data, train_target = split_target(train_data)\n",
    "\n",
    "\n",
    "train_data_dropped_cat = train_data.select_dtypes(exclude=['object'])\n",
    "validation_dropped_cat = validation_data.select_dtypes(exclude=['object'])\n",
    "\n",
    "train_data_2, train_validation_data, train_target_2 , train_validation_target = train_test_split(train_data_dropped_cat, train_target, train_size = 0.8, test_size = 0.2, random_state = 0)\n",
    "\n",
    "\n",
    "    \n",
    "#Model creating\n",
    "    \n",
    "'''\n",
    "def objective(trial):\n",
    "    xgb_params = dict(\n",
    "        max_depth=trial.suggest_int(\"max_depth\", 2, 10),\n",
    "        learning_rate=trial.suggest_float(\"learning_rate\", 1e-4, 1e-1, log=True),\n",
    "        n_estimators=trial.suggest_int(\"n_estimators\", 1000, 8000),\n",
    "        min_child_weight=trial.suggest_int(\"min_child_weight\", 1, 10),\n",
    "        colsample_bytree=trial.suggest_float(\"colsample_bytree\", 0.2, 1.0),\n",
    "        subsample=trial.suggest_float(\"subsample\", 0.2, 1.0),\n",
    "        reg_alpha=trial.suggest_float(\"reg_alpha\", 1e-4, 1e2, log=True),\n",
    "        reg_lambda=trial.suggest_float(\"reg_lambda\", 1e-4, 1e2, log=True),\n",
    "    )\n",
    "    xgb = XGBRegressor(**xgb_params)\n",
    "    #return None\n",
    "    return score_dataset(train_data_2, train_target_2, xgb)\n",
    "\n",
    "\n",
    "\n",
    "study = optuna.create_study(direction=\"minimize\")\n",
    "study.optimize(objective, n_trials=20)\n",
    "xgb_params = study.best_params'''\n",
    "\n",
    "\n",
    "predict_model = XGBRegressor(n_estimators = 500, learning_rate = 0.01)\n",
    "    \n",
    "\n",
    "my_pipeline = Pipeline(steps=[('model', predict_model)])\n",
    "\n",
    "my_pipeline.fit(train_data_2, train_target_2)\n",
    "\n",
    "preds = predict_model.predict(train_validation_data)\n",
    "\n",
    "mae = mean_absolute_error(preds, train_validation_target)\n",
    "\n",
    "\n",
    "final_preds = predict_model.predict(validation_dropped_cat)\n",
    "output = pd.DataFrame({'id': validation_data.index,\n",
    "                       'loan_paid_back': final_preds})\n",
    "output.to_csv('submission.csv', index=False)\n"
   ]
